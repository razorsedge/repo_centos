{
  "name": "flakrat-repo_centos",
  "version": "1.0.0",
  "author": "flakrat",
  "summary": "Manage the CentOS YUM repositories on CentOS clients.",
  "license": "Apache License, Version 2.0",
  "project_page": "https://github.com/flakrat/repo_centos",
  "issues_url": "https://github.com/flakrat/repo_centos/issues",
